<table>
	<thead>
		<tr>
			<th>Modules</th>
		</tr>
	</thead>
	<tbody>
	    <% @nops.each_index do |i| 
				m = @nops[i]
		%>
		
		<tr class="itemTitle">
			<td class="itemTitle">
			    <a onClick="window.parent.openModuleWindow('nops', '<%= m.refname.gsub('/', ':') %>', '<%= m.name %>')" href="#"><%= m.name %></a>
			</td>
		</tr>
		
		<tr class="itemDescription">
			<td class="itemDescription"><%= m.description %></td>
		</tr>
		
		<% end %>
	</tbody>
</table>
