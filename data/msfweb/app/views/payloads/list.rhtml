<html>
<head>
<title>Payloads</title>
</head>
<body>

<h1>Payloads</h1>
<table border="1">
 <tr>
  <td><p align="center"><i><b>Module</b></i></td>
  <td><p align="center"><i><b>Title</b></i></td>
 </tr>

 <% $framework.payloads.each_module do |path, pcls| 
 	m = pcls.new
 %>
  <tr>
   <td><%= link_to path, :controller => :payloads %></td>
   <td><%= m.name %></td>
  </tr>
 <% end %>
</table>

</body>
</html>
