<table cellpadding="0" cellspacing="0" border="0">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th>Module</th>
		</tr>
	</thead>
	<tbody>
	    <% @all_payloads.each_module do |path, pcls|
	       m = pcls.new
	    %>
		<tr><td><%= m.name %></td><td><%= m.description %></td><td><%= path %></td></tr>
		<% end %>
	</tbody>
</table>