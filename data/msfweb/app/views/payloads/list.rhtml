<table cellpadding="0" cellspacing="0" border="0">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
	    <% @payloads.each_index do |i| 
				m = @payloads[i]
		%>
		<tr>
			<td class="itemTitle">
			    <a onClick="window.parent.openModuleWindow('payloads', '<%= i %>', '<%= m.name %>')" href="#"><%= m.name %></a>
			</td>
			<td class="itemDescription"><%= m.description %></td>
		</tr>
		<% end %>
	</tbody>
</table>
